<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="maxicareactuarial.detail.adjustments.factors100" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Factors">
		<content>
			<ObjectHeader binding="" title="Company Name"
						  number="Quotation No 0001" backgroundDesign="Solid">
				<statuses>
					<ObjectStatus text="Plan ID"/>
				</statuses>
				<attributes>
					<ObjectAttribute text="Company Address" />
					<ObjectAttribute text="Company TIN" />
				</attributes>
			</ObjectHeader>
			<IconTabBar id="idFactorsIconTabBar" select="handleIconTabBarSelect"
						class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter icon="sap-icon://approvals" text="Risk Factors"
									key="RiskFactors">
						<mvc:XMLView viewName="maxicareactuarial.detail.adjustments.tab.factors.riskFactor100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Pricing Adjustment"
									key="PricingAdjustment">
						<mvc:XMLView viewName="maxicareactuarial.detail.adjustments.tab.factors.pricingAdjustment100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Model Factor"
									key="ModelFactor">
						<mvc:XMLView viewName="maxicareactuarial.detail.adjustments.tab.factors.modelFactor100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Substandard Rating"
									key="SubstandardRating">
						<mvc:XMLView viewName="maxicareactuarial.detail.adjustments.tab.factors.substandardRating100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Dependent Unit"
									key="DependentUnit">
						<mvc:XMLView viewName="maxicareactuarial.detail.adjustments.tab.factors.dependentUnit100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Uniform Rates"
									key="UniformRates">
						<mvc:XMLView viewName="maxicareactuarial.detail.adjustments.tab.factors.uniformRates100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Riders"
									key="Riders">
						<mvc:XMLView viewName="maxicareactuarial.detail.adjustments.tab.factors.riders100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Remarks"
									key="Remarks">
						<mvc:XMLView viewName="maxicareactuarial.detail.adjustments.tab.factors.remarks100"/>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
	</Page>
</core:View>