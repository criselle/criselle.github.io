<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form" 
		controllerName="maxicareactuarial.detail.adjustments.tab.factors.riders100" 
		xmlns:html="http://www.w3.org/1999/xhtml">
	<VBox class="sapUiSmallMargin">
		<Panel expandable="true" expanded="true" id="idRidersPanel" 
				width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Riders"/>
				</Toolbar>
			</headerToolbar>
			<Table id="idRidersTable" inset="false" items="">
				<columns>
					<Column hAlign="Center">
						<Text text="Availment Rate" />
					</Column>
					<Column hAlign="Center">
						<Text text="Annual Checkup" />
					</Column>
					<Column hAlign="Center">
						<Text text="Dental" />
					</Column>
					<Column hAlign="Center">
						<Text text="Outpatient Medicines" />
					</Column>
					<Column hAlign="Center">
						<Text text="Optical" />
					</Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Text text="Principals:" />
							<Input value="" id="idPrincipalsAnnualCheckInpt" />
							<Input value="" id="idPrincipalsDentalInpt" />
							<Input value="" id="idPrincipalsOutpatientMedInpt" />
							<Input value="" id="idPrincipalsOpticalInpt" />
						</cells>
					</ColumnListItem>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Text text="Dependents:" />
							<Input value="" id="idDepAnnualCheckInpt" />
							<Input value="" id="idDepDentalInpt" />
							<Input value="" id="idDepOutpatientMedInpt" />
							<Input value="" id="idDepOpticalInpt" />
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</Panel>
		<Panel expandable="true" expanded="true" id="idTakeHomeMedsPanel" 
				width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Take-Home Medicines"/>
				</Toolbar>
			</headerToolbar>
			<f:Form id="idTakeHomeMedsForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="Average Cost">
								<Input value="" id="idAveCostInpt"/>
							</f:FormElement>
							<f:FormElement label="Availment Rate">
								<Input value="" id="idAvailRateInpt"/>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
		<Panel expandable="true" expanded="true" id="idExecCheckupPanel" 
				width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Executive Checkup"/>
				</Toolbar>
			</headerToolbar>
			<f:Form id="idExecCheckupForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="Inpatient">
								<Input value="" id="idInpatientInpt"/>
							</f:FormElement>
							<f:FormElement label="Outpatient">
								<Input value="" id="idOutpatientInpt"/>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
		<Panel expandable="true" expanded="true" id="idMaternityPanel" 
				width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Maternity"/>
				</Toolbar>
			</headerToolbar>
			<Table id="idMaternityTable" inset="false" items="">
				<columns>
					<Column hAlign="Center">
						<Text text="Services" />
					</Column>
					<Column hAlign="Center">
						<Text text="Limit" />
					</Column>
					<Column hAlign="Center">
						<Text text="Risk Factor" />
					</Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Text text="Caesarean:"/>
							<Input value="" id="idCaesareanLimitInpt" />
							<Input value="" id="idCaesareanRFInpt" />
						</cells>
					</ColumnListItem>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Text text="Normal Spontaneous Vaginal Delivery/Home:" />
							<Input value="" id="idNormalLimitInpt" />
							<Input value="" id="idNormalRFInpt" />
						</cells>
					</ColumnListItem>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Text text="Home:" />
							<Input value="" id="idHomeLimitInpt" />
							<Input value="" id="idHomeRFInpt" />
						</cells>
					</ColumnListItem>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Text text="Miscarriage:" />
							<Input value="" id="idMiscarriageLimitInpt" />
							<Input value="" id="idMiscarriageRFInpt" />
						</cells>
					</ColumnListItem>
					<ColumnListItem vAlign="Middle">
						<cells>
							<Text text="Others:" />
							<Input value="" id="idOthersLimitInpt" />
							<Input value="" id="idOthersRFInpt" />
						</cells>
					</ColumnListItem>
				</items>
			</Table>
			<f:Form id="idMaternityForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="4"
						labelSpanL="4"
						labelSpanM="4"
						labelSpanS="4"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="Aggregate">
								<Input value="" id="idAggregateLimitInpt">
									<layoutData>
										<l:GridData span="XL4 L4 M4 S4" />
									</layoutData>
								</Input>
							</f:FormElement>
							<f:FormElement label="Availment Rate">
								<Input value="" id="idAvailRateLimitInpt">
									<layoutData>
										<l:GridData span="XL4 L4 M4 S4" />
									</layoutData>
								</Input>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
	</VBox>
</core:View>