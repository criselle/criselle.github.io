<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form" 
		controllerName="maxicareactuarial.detail.adjustments.tab.factors.dependentUnit100" 
		xmlns:html="http://www.w3.org/1999/xhtml">
	<VBox class="sapUiSmallMargin">
		<Panel id="idDependentUnitPanel" width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Dependent Unit"/>
				</Toolbar>
			</headerToolbar>
			<f:Form id="idDependentUnitForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="Maximum Number of Dependents">
								<Input value="" id="idMaxNumOfDepInpt"/>
							</f:FormElement>
							<f:FormElement label="Dependent Unit Factor">
								<Input value="" id="idDepUnitFactorInpt"/>
							</f:FormElement>
							<f:FormElement label="Count of Dependent Unit">
								<Input value="" id="idCountOfDepUnitInpt"/>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
	</VBox>
</core:View>