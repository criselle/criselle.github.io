<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form" 
		controllerName="maxicareactuarial.detail.adjustments.tab.factors.uniformRates100" 
		xmlns:html="http://www.w3.org/1999/xhtml">
	<VBox class="sapUiSmallMargin">
		<Panel id="idUniformRatesPanel" width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Uniform Rates"/>
				</Toolbar>
			</headerToolbar>
			<f:Form id="idUniformRatesForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="Principals">
								<Input value="" id="idPrincipalsInpt"/>
							</f:FormElement>
							<f:FormElement label="Dependents">
								<Input value="" id="idDependentsInpt"/>
							</f:FormElement>
							<f:FormElement label="Use Actual Count per Plan">
								<Switch id="idUseActualCountSwitch" state="false" customTextOn="Yes"
										customTextOff="No" change="onChangeUseActualCoun"/>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
	</VBox>
</core:View>