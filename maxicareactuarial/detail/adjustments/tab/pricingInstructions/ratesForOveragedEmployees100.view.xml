<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form"
		controllerName="maxicareactuarial.detail.adjustments.tab.pricingInstructions.ratesForOveragedEmployees100" 
		xmlns:html="http://www.w3.org/1999/xhtml">
	<VBox class="sapUiSmallMargin">
		<Panel id="idRatesForOveragedEmployeesPanel1" width="auto">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Rates for Overaged Employees"/>
				</Toolbar>
			</headerToolbar>
			<f:Form id="idRatesForOveragedEmployeesForm1" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement>
								<RadioButton groupName="RatesGroupA" text="Fee-for-Service">
									<layoutData>
										<l:GridData indent="XL1 L1 M1 S0" span="XL3 L3 M3 S4" />
									</layoutData>
								</RadioButton>
								<RadioButton groupName="RatesGroupA" text="Substandard Premium">
									<layoutData>
										<l:GridData span="XL3 L3 M3 S4" />
									</layoutData>
								</RadioButton>
								<RadioButton groupName="RatesGroupA" text="Standard Premium">
									<layoutData>
										<l:GridData span="XL3 L3 M3 S4" />
									</layoutData>
								</RadioButton>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
		<Panel id="idRatesForOveragedEmployeesPanel2" width="auto">
			<f:Form id="idRatesForOveragedEmployeesForm2" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement>
								<RadioButton groupName="RatesGroupB" text="Charged to Account">
									<layoutData>
										<l:GridData indent="XL1 L1 M1 S0" span="XL3 L3 M3 S4" />
									</layoutData>
								</RadioButton>
								<RadioButton groupName="RatesGroupB" text="Absorb to CM">
									<layoutData>
										<l:GridData span="XL3 L3 M3 S4" />
									</layoutData>
								</RadioButton>
								<RadioButton groupName="RatesGroupB" text="Absorb to Commission">
									<layoutData>
										<l:GridData span="XL3 L3 M3 S4" />
									</layoutData>
								</RadioButton>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
	</VBox>
</core:View>