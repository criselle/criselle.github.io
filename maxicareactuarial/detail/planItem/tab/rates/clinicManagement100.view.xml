<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form"
		controllerName="maxicareactuarial.detail.planItem.tab.rates.clinicManagement100" 
		xmlns:html="http://www.w3.org/1999/xhtml">
	<VBox class="sapUiSmallMargin">
		<Panel id="idClinicManagementPanel" width="auto">
			<f:Form id="idClinicManagementForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement>
								<Button id="idRefreshBtn" text="Refresh" press="onPressRefresh">
									<layoutData>
										<l:GridData indent="XL3 L3 M3 S0" span="XL3 L3 M3 S6" />
									</layoutData>
								</Button>
								<Button id="idEditBtn" text="Edit" press="onPressEdit">
									<layoutData>
										<l:GridData span="XL3 L3 M3 S6" />
									</layoutData>
								</Button>
							</f:FormElement>
							<f:FormElement label="Contribution Margin">
								<Input value="" id="idContributionMargin1Inpt"/>
							</f:FormElement>
							<f:FormElement label="Personnel">
								<Input value="" id="idPersonnelInpt"/>
							</f:FormElement>
							<f:FormElement label="Specialization">
								<Input value="" id="idSpecializationInpt"/>
							</f:FormElement>
							<f:FormElement label="No. of Personnel">
								<Input value="" id="idNumOfPersonnelInpt"/>
							</f:FormElement>
							<f:FormElement label="Type of Payment">
								<Select id="idTypeOfPaymentSelect" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
								</Select>
							</f:FormElement>
							<f:FormElement label="Amount">
								<Input value="" id="idAmountInpt" type="Number"/>
							</f:FormElement>
							<f:FormElement label="Mode of Payment">
								<Select id="idModeOfPaymentSelect" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
								</Select>
							</f:FormElement>
							<f:FormElement label="Contribution Margin">
								<Input value="" id="idContributionMargin2Inpt"/>
							</f:FormElement>
							<f:FormElement label="Commission">
								<Select id="idCommissionSelect1" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Select>
								<Input value="" id="idCommissionInpt">
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Input>
								<Select id="idCommissionSelect2" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Select>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
					<f:FormContainer title="Agency Level Set-up">
						<f:formElements>
							<f:FormElement label="Health Benefit Agent">
								<Select id="idHealthBenAgentSelect1" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Select>
								<Input value="" id="idHealthBenAgentInpt">
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Input>
								<Select id="idHealthBenAgentSelect2" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Select>
							</f:FormElement>
							<f:FormElement label="Unit Manager">
								<Select id="idUnitMngrSelect1" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Select>
								<Input value="" id="idUnitMngrInpt">
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Input>
								<Select id="idUnitMngrSelect2" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Select>
							</f:FormElement>
							<f:FormElement label="Unit Head">
								<Select id="idUnitHeadSelect1" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Select>
								<Input value="" id="idUnitHeadInpt">
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Input>
								<Select id="idUnitHeadSelect2" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL2 L2 M2 S4" />
									</layoutData>
								</Select>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
	</VBox>
</core:View>