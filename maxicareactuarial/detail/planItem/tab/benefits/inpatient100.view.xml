<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form"
		controllerName="maxicareactuarial.detail.planItem.tab.benefits.inpatient100" 
		xmlns:html="http://www.w3.org/1999/xhtml">
	<VBox class="sapUiSmallMargin">
		<List items="{benefitsModel>/benefits}" >
			<CustomListItem>
				<VBox>
					<HBox>
						<Text text="{benefitsModel>number}" class="custom-numbering"/>
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
							<Text width="100%" text="{benefitsModel>text}"/>
						</VBox>
					</HBox>
					<f:Form editable="true">
						<f:layout>
							<f:ResponsiveGridLayout
								labelSpanXL="3"
								labelSpanL="3"
								labelSpanM="3"
								labelSpanS="12"
								adjustLabelSpan="false"
								emptySpanXL="3"
								emptySpanL="3"
								emptySpanM="3"
								emptySpanS="0"
								columnsXL="1"
								columnsL="1"
								columnsM="1"
								singleContainerFullSize="false" />
						</f:layout>
						<f:formContainers>
							<f:FormContainer>
								<f:formElements>
									<f:FormElement label="Limit">
										<Input value="">
											<layoutData>
												<l:GridData span="XL3 L3 M3 S6" />
											</layoutData>
										</Input>
										<Select selectedKey="">
											<items>
												<core:Item text="" key=""/>
											</items>
											<layoutData>
												<l:GridData span="XL3 L3 M3 S6" />
											</layoutData>
										</Select>
									</f:FormElement>
									<f:FormElement label="Member Share">
										<Input value="">
											<layoutData>
												<l:GridData span="XL2 L2 M2 S4" />
											</layoutData>
										</Input>
										<Select selectedKey="">
											<items>
												<core:Item text="" key=""/>
											</items>
											<layoutData>
												<l:GridData span="XL2 L2 M2 S4" />
											</layoutData>
										</Select>
										<Select selectedKey="">
											<items>
												<core:Item text="" key=""/>
											</items>
											<layoutData>
												<l:GridData span="XL2 L2 M2 S4" />
											</layoutData>
										</Select>
									</f:FormElement>
								</f:formElements>
							</f:FormContainer>
						</f:formContainers>
					</f:Form>
				</VBox>
			</CustomListItem>
		</List>
	</VBox>
</core:View>