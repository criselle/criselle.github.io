						<mvc:XMLView viewName="maxicareactuarial.detail.planItem.tab.riders.groupLife100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Dental"
									key="Dental">
						<mvc:XMLView viewName="maxicareactuarial.detail.planItem.tab.riders.dental100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Cancer Benefits"
