					<IconTabFilter icon="sap-icon://approvals" text="Optical"
									key="Optical">
						<mvc:XMLView viewName="maxicareactuarial.detail.planItem.tab.riders.optical100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="List of Doctors"
									key="ListOfDoctors">
						<mvc:XMLView viewName="maxicareactuarial.detail.planItem.tab.riders.listOfDoctors100"/>
