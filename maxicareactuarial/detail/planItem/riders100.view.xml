<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="maxicareactuarial.detail.planItem.riders100" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Riders">
		<content>
			<ObjectHeader binding="" title="Company Name"
