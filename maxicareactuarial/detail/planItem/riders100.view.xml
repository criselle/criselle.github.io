				</attributes>
			</ObjectHeader>
			<IconTabBar id="idRidersIconTabBar" select="handleIconTabBarSelect"
						class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter icon="sap-icon://approvals" text="Annual Checkup"
									key="AnnualCheckup">
						<mvc:XMLView viewName="maxicareactuarial.detail.planItem.tab.riders.annualCheckup100"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://approvals" text="Pre-employment Exam"
									key="PreemploymentExam">
