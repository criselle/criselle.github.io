<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form" controllerName="maxicareactuarial.detail.quotationItem.tab.otherArrangement.claims100"
		xmlns:html="http://www.w3.org/1999/xhtml">
	<VBox class="sapUiSmallMargin">
		<Panel id="idClaimsPanel" width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Claims"/>
				</Toolbar>
			</headerToolbar>
			<f:Form id="ClaimsForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="Claims Reimbursement Filing Period">
								<Input value="" id="idClaimsRFPInpt">
									<layoutData>
										<l:GridData span="XL3 L3 M3 S6" />
									</layoutData>
								</Input>
								<Select id="idClaimsRFPeriodSelect" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL3 L3 M3 S6" />
									</layoutData>
								</Select>
							</f:FormElement>
							<f:FormElement label="TAT for Claims Processing">
								<Input value="" id="idTATForClaimsProcessingInpt">
									<layoutData>
										<l:GridData span="XL3 L3 M3 S6" />
									</layoutData>
								</Input>
								<Select id="idTATForClaimsProcessingSelect" selectedKey="">
									<items>
										<core:Item text="" key=""/>
									</items>
									<layoutData>
										<l:GridData span="XL3 L3 M3 S6" />
									</layoutData>
								</Select>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
	</VBox>
</core:View>