<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form" controllerName="maxicareactuarial.detail.quotationItem.tab.otherArrangement.lockInCoverage100"
		xmlns:html="http://www.w3.org/1999/xhtml">
	<VBox class="sapUiSmallMargin">
		<Panel expandable="true" expanded="true" id="idRenewalEscalationPanel" width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Renewal Escalation"/>
				</Toolbar>
			</headerToolbar>
			<f:Form id="RenewalEscalationForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="Loss Ratio (1st Year)">
								<Input value="" id="idLossRatioInpt"/>
							</f:FormElement>
							<f:FormElement label="Increase/Decrease">
								<Input value="" id="idIncreaseDecreaseInpt"/>
							</f:FormElement>
							<f:FormElement label="CM">
								<Input value="" id="idCMInpt"/>
								<Button id="idAddCMBtn" icon="sap-icon://add">
									<layoutData>
										<l:GridData span="XL1 L1 M1 S6" />
									</layoutData>
								</Button>
								<Button id="idDeleteCMBtn" icon="sap-icon://delete">
									<layoutData>
										<l:GridData span="XL1 L1 M1 S6" />
									</layoutData>
								</Button>
							</f:FormElement>
							<f:FormElement>
								<Table id="idRenewalEscalationTable" inset="false" items="">
										<columns>
											<Column hAlign="Center">
												<Text text="Loss Ratio" />
											</Column>
											<Column hAlign="Center">
												<Text text="Increase/Decrease" />
											</Column>
											<Column hAlign="Center">
												<Text text="CM" />
											</Column>
										</columns>
										<items>
											<ColumnListItem>
												<cells>
													<Text
														text="" />
													<Text
														text="" />
													<Text
													text="" />
												</cells>
											</ColumnListItem>
										</items>
										<layoutData>
											<l:GridData indent="XL3 L3 M3 S0" span="XL6 L6 M6 S12"/>
										</layoutData>
									</Table>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
		<Panel expandable="true" expanded="true" id="idProvisionFRPanel" width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Provision (Full-Risk)"/>
				</Toolbar>
			</headerToolbar>
			<f:Form id="ProvisionFRForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="Applicable">
								<Switch id="idApplicableProvFRSwitch" state="false" customTextOn="Yes"
										customTextOff="No" change="onChangeApplicableProvFR">
									<layoutData>
										<l:GridData span="XL3 L3 M3 S6" />
										<FlexItemData growFactor="1" />
									</layoutData>
								</Switch>
								<FormattedText htmlText="{TextArea>/ProvFRText}">
									<layoutData>
										<l:GridData span="XL12 L12 M12 S12" />
									</layoutData>
								</FormattedText>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
		<Panel expandable="true" expanded="true" id="idProvisionASOPanel" width="auto" class="custom-panel-bottom">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Provision (ASO)"/>
				</Toolbar>
			</headerToolbar>
			<f:Form id="ProvisionASOForm" editable="true">
				<f:layout>
					<f:ResponsiveGridLayout
						labelSpanXL="3"
						labelSpanL="3"
						labelSpanM="3"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="3"
						emptySpanL="3"
						emptySpanM="3"
						emptySpanS="0"
						columnsXL="1"
						columnsL="1"
						columnsM="1"
						singleContainerFullSize="false" />
				</f:layout>
				<f:formContainers>
					<f:FormContainer>
						<f:formElements>
							<f:FormElement label="Applicable">
								<Switch id="idApplicableProvASOSwitch" state="false" customTextOn="Yes"
										customTextOff="No" change="onChangeApplicableProvASO">
									<layoutData>
										<l:GridData span="XL3 L3 M3 S6" />
										<FlexItemData growFactor="1" />
									</layoutData>
								</Switch>
								<FormattedText htmlText="{TextArea>/ProvFRText}">
									<layoutData>
										<l:GridData span="XL12 L12 M12 S12" />
									</layoutData>
								</FormattedText>
							</f:FormElement>
						</f:formElements>
					</f:FormContainer>
				</f:formContainers>
			</f:Form>
		</Panel>
	</VBox>
</core:View>