<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
		xmlns:u="sap.ui.unified" controllerName="maxicareactuarial.master.adjustments100" 
		xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Adjustments" navButtonPress="onPressNavBack" showNavButton="true">
		<content>
			<List itemPress="onListItemPress">
				<items>
					<StandardListItem id="Factors" title="Factors" type="Active"/>
					<StandardListItem id="Network" title="Network" type="Active"/>
					<StandardListItem id="Benefits" title="Benefits" type="Active"/>
					<StandardListItem id="ClaimsAnalysis" title="Claims Analysis" type="Active"/>
				</items>
			</List>
		</content>
		<footer>
			<Bar>
				<contentRight>
					<Button text="Save" press="onPressSave"/>
					<Button text="Cancel" press="onPressCancel"/>
				</contentRight>
			</Bar>
		</footer>
	</Page>
</core:View>