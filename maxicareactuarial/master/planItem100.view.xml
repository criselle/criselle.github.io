<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="maxicareactuarial.master.planItem100" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Products" navButtonPress="onPressNavBack" showNavButton="true">
		<content>
			<List itemPress="onListItemPress">
				<items>
					<StandardListItem id="PlanDetails" title="Plan Details" type="Active"/>
					<StandardListItem id="MembershipEligibility" title="Membership Eligibility" type="Active"/>
					<StandardListItem id="SalientFeatures" title="Salient Features" type="Active"/>
					<StandardListItem id="DailyCashAllowance" title="Daily Cash Allowance" type="Active"/>
					<StandardListItem id="ProviderAccess" title="Provider Access" type="Active"/>
					<StandardListItem id="MemberShare" title="Member Share" type="Active"/>
					<StandardListItem id="Benefits" title="Benefits" type="Active"/>
					<StandardListItem id="Riders" title="Riders" type="Active"/>
					<StandardListItem id="ExclusionsAndLimitations" title="Exclusions and Limitations" type="Active"/>
					<StandardListItem id="MedicalInsurance" title="Medical Insurance" type="Active"/>
					<StandardListItem id="Rates" title="Rates" type="Active"/>
					<StandardListItem id="Adjustments" title="Adjustments" type="Active"/>
				</items>
			</List>
		</content>
		<footer>
			<Bar>
				<contentRight>
					<Button text="Save" press="onPressSave"/>
					<Button text="Cancel" press="onPressCancel"/>
				</contentRight>
			</Bar>
		</footer>
	</Page>
</core:View>